app_key: your_app_key_here
auth:
  access_token: your_access_token_here
  expires_at: 0
  refresh_token: your_refresh_token_here
clean:
  duplicate_keep: keep_shortest_path
  exclude_dirs:
  - /apps
  - /我的资源
  expire_days: 365
  large_file_threshold_mb: 500
organize:
  date_rules:
  - extensions:
    - .jpg
    - .jpeg
    - .png
    - .heic
    - .mov
    - .mp4
    source_dir: /来自：iPhone
    target_pattern: /照片/{year}/{month}
  - extensions:
    - .jpg
    - .jpeg
    - .png
    - .heic
    - .mov
    - .mp4
    source_dir: /相机胶卷
    target_pattern: /照片/{year}/{month}
  enabled: true
  exclude_dirs:
  - /apps
  - /我的资源
  - /.recycle
  keyword_rules:
  - keyword: 合同
    target: /工作/合同
  - keyword: 发票
    target: /工作/发票
  - keyword: 简历
    target: /工作/简历
  - keyword: 报告
    target: /工作/报告
  source_dir: /
  type_rules:
    压缩包:
      extensions:
      - .zip
      - .rar
      - .7z
      - .tar
      - .gz
      - .bz2
      - .xz
      target: /压缩包
    图片:
      extensions:
      - .jpg
      - .jpeg
      - .png
      - .gif
      - .bmp
      - .svg
      - .webp
      - .tiff
      - .raw
      - .heic
      target: /图片
    安装包:
      extensions:
      - .exe
      - .msi
      - .dmg
      - .pkg
      - .deb
      - .rpm
      - .apk
      - .ipa
      target: /安装包
    文档:
      extensions:
      - .doc
      - .docx
      - .pdf
      - .txt
      - .md
      - .xlsx
      - .xls
      - .pptx
      - .ppt
      - .csv
      - .rtf
      - .odt
      target: /文档
    视频:
      extensions:
      - .mp4
      - .avi
      - .mkv
      - .mov
      - .wmv
      - .flv
      - .rmvb
      - .rm
      - .3gp
      - .ts
      target: /视频
    音乐:
      extensions:
      - .mp3
      - .wav
      - .flac
      - .aac
      - .ogg
      - .wma
      - .ape
      - .m4a
      target: /音乐
scan:
  exclude_dirs:
  - /.recycle
  max_depth: 0
  root_dir: /
secret_key: your_secret_key_here
sync:
  exclude_patterns:
  - '*.tmp'
  - '*.DS_Store'
  - Thumbs.db
  local_dir: ~/baidu-backup
  max_files: 1000
  remote_dir: /同步备份

